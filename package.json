{
  "name": "@echamudi/minimath",
  "version": "1.0.0",
  "description": "JavaScript module template for node.js and web browser environment",
  "main": "./index.js",
  "private": "false",
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.build-commonjs.json",
    "build:umd": "webpack --config webpack.build-umd.js --mode development",
    "build:umd-min": "webpack --config webpack.build-umd.js --mode production",
    "build": "run-s build:commonjs build:umd build:umd-min",
    "test:browser": "node ./test/puppeteer-run.js",
    "test:node": "mocha test/test.node.js",
    "test": "run-s test:browser test:node"
  },
  "author": "Ezat Chamudi <echamudi@outlook.com> (https://github.com/echamudi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/echamudi/minimath.git"
  },
  "bugs": {
    "url": "https://github.com/echamudi/minimath/issues"
  },
  "homepage": "https://github.com/echamudi/minimath#readme",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "babel-loader": "^8.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^4.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
