{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off"
    }
}
